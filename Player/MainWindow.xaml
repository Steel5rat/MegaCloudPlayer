<Window x:Class="Player.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="476*"/>
            <ColumnDefinition Width="41*"/>
        </Grid.ColumnDefinitions>
        <ListBox HorizontalAlignment="Left" Height="320" VerticalAlignment="Top" Width="258" Name="NotPlayingList" MouseDoubleClick="NotPlayingList_OnMouseDoubleClick"/>
        <ListBox HorizontalAlignment="Left" Height="320" Margin="258,0,0,0" VerticalAlignment="Top" Width="259" Name="PlayingList" MouseDoubleClick="PlayingList_OnMouseDoubleClick" Grid.ColumnSpan="2"/>
    </Grid>
</Window>
